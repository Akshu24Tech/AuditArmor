{
  "extract": {
    "json_schema": {
      "type": "object",
      "properties": {
        "conflicts": {
          "type": "array",
          "description": "List of compliance conflicts found between the regulation and company policy",
          "items": {
            "type": "object",
            "properties": {
              "conflict_summary": {
                "type": "string",
                "description": "Brief description of the conflict or compliance gap"
              },
              "regulation_page": {
                "type": "integer",
                "description": "Page number in the government regulation document"
              },
              "regulation_text": {
                "type": "string",
                "description": "Relevant text snippet from the government regulation"
              },
              "policy_page": {
                "type": ["integer", "null"],
                "description": "Page number in the company policy document, or null if requirement is missing entirely"
              },
              "policy_text": {
                "type": ["string", "null"],
                "description": "Relevant text snippet from the company policy, or null if requirement is missing"
              },
              "severity": {
                "type": "string",
                "enum": ["critical", "major", "minor"],
                "description": "Severity level of the compliance gap"
              },
              "recommendation": {
                "type": "string",
                "description": "Suggested action to resolve the compliance gap"
              }
            },
            "required": ["conflict_summary", "regulation_page", "regulation_text", "policy_page", "policy_text", "severity", "recommendation"]
          }
        },
        "total_conflicts": {
          "type": "integer",
          "description": "Total number of compliance conflicts identified"
        },
        "critical_count": {
          "type": "integer",
          "description": "Number of critical severity conflicts"
        },
        "major_count": {
          "type": "integer",
          "description": "Number of major severity conflicts"
        },
        "minor_count": {
          "type": "integer",
          "description": "Number of minor severity conflicts"
        }
      },
      "required": ["conflicts", "total_conflicts", "critical_count", "major_count", "minor_count"]
    },
    "settings": {
      "extraction_mode": "PREMIUM",
      "system_prompt": null,
      "citation_bbox": true,
      "use_reasoning": false,
      "cite_sources": true,
      "confidence_scores": true
    }
  },
  "parse": {
    "settings": {
      "tier": "agentic",
      "version": "latest"
    }
  },
  "split": {
    "categories": [],
    "settings": {}
  },
  "classify": {
    "rules": [],
    "settings": {}
  }
}
